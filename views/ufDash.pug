doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  link(rel='stylesheet' href='https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css' integrity='sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm' crossorigin='anonymous')
  link(rel='stylesheet' href='https://www.w3schools.com/w3css/4/w3.css')
  link(rel='stylesheet' href='/bootstrap/css/bootstrap.css')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Urban Farmer Dashboard
.container-fluid
  #mySidebar.w3-sidebar.w3-bar-block.w3-border-right
    button.w3-bar-item.w3-large(onclick='w3_close()') Close &times;
    a.w3-bar-item.w3-button(href='/upload') Upload Produce
    a.w3-bar-item.w3-button(href='#') Bookings and Orders
    a.w3-bar-item.w3-button(href='#') Log out
  .w3-green
    button.w3-button.w3-green.w3-xlarge(onclick='w3_open()') &#x2630;
  fieldset
    legend
      center
        h2
          b Urban Farmer Dashboard
  br
  if message
        .alert.alert-success= message
  
  form(method="POST" action='/logout')
    button.btn(type= 'submit' width='100px') Signout

script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js')
